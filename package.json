{
	"dependencies": {
		"@apiture/openapi-down-convert": "^0.9.0",
		"@cfworker/json-schema": "^1.12.7",
		"@redocly/cli": "^1.6.0",
		"oatts": "^1.6.0",
		"openapi-types": "^12.1.3",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@types/jest": "^29.5.11",
		"@types/node": "^20.10.8",
		"@types/uuid": "^9.0.7",
		"axios": "^1.6.5",
		"jest": "^29.7.0",
		"jest-openapi": "^0.14.2",
		"ts-jest": "^29.1.1",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	},
	"main": "./src/index.ts",
	"packageManager": "yarn@4.0.2",
	"scripts": {
		"build": "rm -rfv ./src/generated/clients",
		"generate": "npm run build && npm run generate:spec && npm run generate:typescript",
		"generate:spec": "npx -y tsx ./src/index.ts",
		"generate:typescript": "npx -y @openapitools/openapi-generator-cli generate",
		"refactor": "tsc . && npx -y openapi-refactorer -i 'openapi-spec - JSON.json' -o main.json",
		"test": "jest"
	}
}
