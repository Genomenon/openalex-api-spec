{
	"dependencies": {
		"@apidevtools/json-schema-ref-parser": "^11.1.0",
		"@apiture/openapi-down-convert": "^0.9.0",
		"@cfworker/json-schema": "^1.12.7",
		"@hyperjump/browser": "^1.1.0",
		"@hyperjump/json-schema": "^1.7.0",
		"@redocly/cli": "^1.6.0",
		"allof-merge": "^0.6.4",
		"express-openapi-validator": "github:Mearman/express-openapi-validator#feat/openapi-3.1-support",
		"fets": "^0.7.1",
		"jest-openapi": "^0.14.2",
		"json-schema-library": "^9.1.2",
		"json-schema-to-ts": "^3.0.0",
		"oas-schemas": "git+https://github.com/OAI/OpenAPI-Specification",
		"oatts": "^1.6.0",
		"openapi-schema-validator": "^12.1.3",
		"uuid": "^9.0.1"
	},
	"devDependencies": {
		"@types/node": "^20.10.6",
		"@types/uuid": "^9.0.7",
		"openapi-types": "^12.1.3",
		"tsx": "^4.7.0",
		"typescript": "^5.3.3"
	},
	"scripts": {
		"generate:spec": "npx -y tsx ./src/index.ts",
		"generate:typescript": "npx -y @openapitools/openapi-generator-cli generate",
		"build": "rm -rfv ./src/generated/clients",
		"generate": "npm run build && npm run generate:spec && npm run generate:typescript",
		"refactor": "tsc . && npx -y openapi-refactorer -i 'openapi-spec - JSON.json' -o main.json"
	},
	"main": "./src/index.ts"
}
